{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"customTemplates": [
		{
			"name": "blank",
			"postTypes": [
				"page",
				"post"
			],
			"title": "Blank"
		},
		{
			"name": "blog-alternative",
			"postTypes": [
				"page"
			],
			"title": "Blog (Alternative)"
		},
		{
			"name": "404",
			"postTypes": [
				"page"
			],
			"title": "404"
		}
	],
	"settings": {
		"appearanceTools": true,
		"color": {
			"gradients": [
				{
					"gradient": "linear-gradient(90deg, var(--wp--preset--color--accent-light-10) 0%, var(--wp--preset--color--accent-light-20) 25%, var(--wp--preset--color--accent-light-20) 75%, var(--wp--preset--color--accent-light-10) 100%)",
					"name": "Linear Light Primary",
					"slug": "linear-light-primary"
				}
			],
			"palette": [
				{
					"slug": "neutral-light-constrast-0",
          "color": "#ffffff",
          "name": "Neutral Light Contrast 0"
				},
				{
					"slug": "neutral-light-constrast-10",
          "color": "#F0F2F5",
          "name": "Neutral Light Contrast 10"
				},
				{
					"slug": "neutral-light-light",
          "color": "#f9f7f1",
          "name": "Neutral Light Light"
				},
				{
					"slug": "neutral-light-muted",
          "color": "#a6a6a6",
          "name": "Neutral Light Muted"
				},
				{
					"slug": "neutral-light-body",
          "color": "#2b2d42",
          "name": "Neutral Light Body"
				},
				{
					"slug": "neutral-light-constrast-100",
          "color": "#000000",
          "name": "Neutral Light Constrast 100"
				},
				{
					"slug": "neutral-dark-constrast-0",
          "color": "#000000",
          "name": "Neutral Dark Contrast 0"
				},
				{
					"slug": "neutral-dark-constrast-10",
          "color": "#F0F2F5",
          "name": "Neutral Dark Contrast 10"
				},
				{
					"slug": "neutral-dark-light",
          "color": "#3A3C3F",
          "name": "Neutral Dark Light"
				},
				{
					"slug": "neutral-dark-muted",
          "color": "#8d99ae",
          "name": "Neutral Dark Muted"
				},
				{
					"slug": "neutral-dark-body",
          "color": "#f9f7f1",
          "name": "Neutral Dark Body"
				},
				{
					"slug": "neutral-dark-constrast-100",
          "color": "#ffffff",
          "name": "Neutral Dark Constrast 100"
				},
				{
					"slug": "accent-light-10",
          "color": "#6FB270",
          "name": "Accent Light 10"
				},
				{
					"slug": "accent-light-20",
          "color": "#015B3F",
          "name": "Accent Light 20"
				},
				{
					"slug": "accent-light-30",
          "color": "#859E3B",
          "name": "Accent Light 30"
				},
				{
					"slug": "accent-light-40",
          "color": "#ED6234",
          "name": "Accent Light 40"
				},
				{
					"slug": "accent-dark-10",
          "color": "#015B3F",
          "name": "Accent Dark 10"
				},
				{
					"slug": "accent-dark-20",
          "color": "#6FB270",
          "name": "Accent Dark 20"
				},
				{
					"slug": "accent-dark-30",
          "color": "#859E3B",
          "name": "Accent Dark 30"
				},
				{
					"slug": "accent-dark-40",
          "color": "#ED6234",
          "name": "Accent Dark 40"
				}
			]
		},
		"layout": {
			"contentSize": "1200px",
			"wideSize": "1600px"
		},
		"spacing": {
			"blockGap": true,
			"margin": true,
			"padding": true,
			"spacingScale": {
				"steps": 0,
				"increment": 1
			},
			"spacingSizes": [
				{
					"size": "clamp(1.5rem, 5vw, 2rem)",
					"slug": "30",
					"name": "1"
				},
				{
					"size": "clamp(1.8rem, 1.8rem + ((1vw - 0.48rem) * 2.885), 3rem)",
					"slug": "40",
					"name": "2"
				},
				{
					"size": "clamp(2.5rem, 8vw, 4.5rem)",
					"slug": "50",
					"name": "3"
				},
				{
					"size": "clamp(3.75rem, 10vw, 7rem)",
					"slug": "60",
					"name": "4"
				},
				{
					"size": "clamp(5rem, 5.25rem + ((1vw - 0.48rem) * 9.096), 8rem)",
					"slug": "70",
					"name": "5"
				},
				{
					"size": "clamp(7rem, 14vw, 11rem)",
					"slug": "80",
					"name": "6"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "\"Montserrat\", sans-serif",
					"name": "Montserrat",
					"slug": "montserrat",
					"fontFace": [
						{
							"fontFamily": "Montserrat",
							"fontWeight": "100",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/Montserrat/static/Montserrat-Thin.ttf" ]
						},
						{
							"fontFamily": "Montserrat",
							"fontWeight": "500",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/Montserrat/static/Montserrat-Medium.ttf" ]
						},
						{
							"fontFamily": "Montserrat",
							"fontWeight": "800",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/Montserrat/static/Montserrat-ExtraBold.ttf" ]
						}
					]
				},
				{
					"fontFamily": "\"Noto Serif\", serif",
					"name": "Noto Serif",
					"slug": "noto-serif",
					"fontFace": [
						{
							"fontFamily": "Noto Serif",
							"fontWeight": "400",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/Noto_Serif/NotoSerif-Regular.ttf" ]
						},
						{
							"fontFamily": "Noto Serif",
							"fontWeight": "700",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/Noto_Serif/NotoSerif-Bold.ttf" ]
						}
					]
				}
			],
			"fontSizes": [
				{
					"fluid": {
						"min": "0.875rem",
						"max": "1rem"
					},
					"size": "1rem",
					"slug": "small"
				},
				{
					"fluid": {
						"min": "1.1rem",
						"max": "1.225rem"
					},
					"size": "1.25rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"min": "1.75rem",
						"max": "1.875rem"
					},
					"size": "1.75rem",
					"slug": "large"
				},
				{
					"fluid": false,
					"size": "2.25rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"min": "4rem",
						"max": "10rem"
					},
					"size": "10rem",
					"slug": "xx-large"
				}
			]
		},
		"useRootPaddingAwareAlignments": false,
		"custom": {
			"transition-general-timing": "0.25s",
			"transition-image-hover": "0.35s"
		}
	},
	"styles": {
		"blocks": {
			"core/navigation": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						":focus": {
							"typography": {
								"textDecoration": "underline dashed"
							}
						},
						":active": {
							"typography": {
								"textDecoration": "none"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-author": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-content": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--neutral-light-body)"
						}
					}
				}
			},
			"core/post-excerpt": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"core/post-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "400"
				},
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						},
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				}
			},
			"core/post-terms": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-title": {
				"spacing": {
					"margin": {
						"bottom": "1.25rem",
						"top": "1.25rem"
					}
				},
				"typography": {
					"fontWeight": "400"
				},
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						":focus": {
							"typography": {
								"textDecoration": "underline dashed"
							}
						},
						":active": {
							"color": {
								"text": "var(--wp--preset--color--neutral-light-body)"
							},
							"typography": {
								"textDecoration": "none"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/comments-title":{
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				},
				"spacing": {
					"margin": {
						"bottom": "var(--wp--preset--spacing--40)"
					}
				}
			},
			"core/comment-author-name": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						":focus": {
							"typography": {
								"textDecoration": "underline dashed"
							}
						},
						":active": {
							"color": {
								"text": "var(--wp--preset--color--neutral-light-body)"
							},
							"typography": {
								"textDecoration": "none"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/comment-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				},
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						":focus": {
							"typography": {
								"textDecoration": "underline dashed"
							}
						},
						":active": {
							"color": {
								"text": "var(--wp--preset--color--neutral-light-body)"
							},
							"typography": {
								"textDecoration": "none"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/comment-edit-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-reply-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comments-pagination": {
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--40)"
					}
				},
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/pullquote": {
				"border": {
					"style": "solid",
					"width": "1px 0"
				},
				"elements": {
					"cite": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--small)",
							"fontStyle": "normal",
							"textTransform": "none"
						}
					}
				},
				"typography": {
					"lineHeight": "1.3"
				},
				"spacing": {
					"margin": {
						"bottom": "var(--wp--preset--spacing--40) !important",
						"top": "var(--wp--preset--spacing--40) !important"
					}
				}
			},
			"core/query": {
				"elements": {
					"h2": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--x-large)"
						}
					}
				}
			},
			"core/query-pagination": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "400"
				},
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						},
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				}
			},
			"core/quote": {
				"border": {
					"width": "1px"
				},
				"elements": {
					"cite": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--small)",
							"fontStyle": "normal"
						}
					}
				},
				"spacing": {
					"padding": {
						"left": "var(--wp--preset--spacing--30)",
						"right": "var(--wp--preset--spacing--30)"
					}
				}
			},
			"core/site-title": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						":focus": {
							"typography": {
								"textDecoration": "underline dashed"
							}
						},
						":active": {
							"color": {
								"text": "var(--wp--preset--color--neutral-light-body)"
							},
							"typography": {
								"textDecoration": "none"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "normal",
					"lineHeight": "1.4"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--neutral-light-light)",
			"text": "var(--wp--preset--color--neutral-light-body)"
		},
		"elements": {
			"button": {
				"border": {
					"radius": "0",
					"width": "1px"
				},
				"color": {
					"background": "var(--wp--preset--color--accent-light-10)",
					"text": "var(--wp--preset--color--neutral-light-body)"
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--accent-light-20)",
						"text": "var(--wp--preset--color--neutral-light-body)"
					}
				},
				":focus": {
					"color": {
						"background": "var(--wp--preset--color--accent-light-20)",
						"text": "var(--wp--preset--color--neutral-light-body)"
					}
				},
				":active": {
					"color": {
						"background": "var(--wp--preset--color--accent-light-10)",
						"text": "var(--wp--preset--color--neutral-light-body)"
					}
				}
			},
			"h1": {
				"typography": {
					"fontSize": "3.625rem",
					"lineHeight": "1.2"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "clamp(2.625rem, calc(2.625rem + ((1vw - 0.48rem) * 8.4135)), 3.25rem)",
					"lineHeight": "1.2"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "700",
					"textTransform": "uppercase"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"textTransform": "uppercase"
				}
			},
			"heading": {
				"typography": {
					"fontWeight": "400",
					"lineHeight": "1.4"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--neutral-light-body)"
				},
				":hover": {
					"typography": {
						"textDecoration": "none"
					}
				},
				":focus": {
					"typography": {
						"textDecoration": "underline dashed"
					}
				},
				":active": {
					"color": {
						"text": "var(--wp--preset--color--neutral-light-body)"
					},
					"typography": {
						"textDecoration": "none"
					}
				},
				"typography": {
					"textDecoration": "underline"
				}
			}
		},
		"spacing": {
			"blockGap": "0rem"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--montserrat)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"lineHeight": "1.6"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		},
		{
			"area": "uncategorized",
			"name": "comments",
			"title": "Comments"
		},
		{
			"area": "uncategorized",
			"name": "post-meta",
			"title": "Post Meta"
		}
	]
}
